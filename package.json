{
  "name": "komercia-core-components",
  "version": "4.3.73",
  "description": "A Vue.js project",
  "author": "Camilo <titanmar15@gmail.com>",
  "private": false,
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "main": "./dist/ko.common.js",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
