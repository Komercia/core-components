    "css-loader": "^3.0.0",
    "element-ui": "^2.4.2",
    "firebase": "^5.11.1",
    "fuse.js": "^3.2.1",
    "image-cloudinary": "^1.2.4",
    "js-cookie": "^2.2.0",
    "komercia-cropper": "^1.0.2",
    "vue": "^2.5.2",
    "vue-awesome-swiper": "^3.1.0",
    "vue-carousel": "^0.6.15",
    "vue-fuse": "^1.5.2",
    "vue-meta": "^1.5.3",
    "vue-mq": "^0.2.1",
    "vue-paginate": "^3.6.0",
    "vue-router": "^3.0.1",
    "vue-social-sharing": "^2.3.3",
    "vue2-animate": "^2.1.0",
